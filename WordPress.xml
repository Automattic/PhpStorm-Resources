    <arrangement>
      <groups>
        <group>
          <type>GETTERS_AND_SETTERS</type>
          <order>KEEP</order>
        </group>
        <group>
          <type>DEPENDENT_METHODS</type>
          <order>DEPTH_FIRST</order>
        </group>
      </groups>
      <rules>
        <rule>
          <match>
            <CONST />
          </match>
        </rule>
        <rule>
          <match>
            <AND>
              <FIELD />
              <PUBLIC />
              <STATIC />
